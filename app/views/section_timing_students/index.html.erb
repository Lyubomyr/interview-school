<p id="notice"><%= notice %></p>

<h1>Section Timing Students</h1>

<table>
  <thead>
    <tr>
      <th>Section timing</th>
      <th>Student</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @section_timing_students.each do |section_timing_student| %>
      <tr>
        <td><%= section_timing_student.section_timing.title %></td>
        <td><%= section_timing_student.student.first_and_last_name %></td>
        <td><%= link_to 'Show', section_timing_student %></td>
        <td><%= link_to 'Edit', edit_section_timing_student_path(section_timing_student) %></td>
        <td><%= link_to 'Destroy', section_timing_student, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Section Timing Student', new_section_timing_student_path %>
